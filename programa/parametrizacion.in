//Ejemplo

compx(u, v : real) : real = (2 + 0.5*cos(v*2*PI))*cos(u*2*PI);

compy(u, v : real) : real = (2 + 0.5*cos(v*2*PI))*sin(u*2*PI);

compz(u, v : real) : real = 0.5*sin(v*2*PI);

g(u, v, t0 : real) : vec3 = vec3(compx(u*cos(t0),v), compy(u,v), compz(u,v));

f(u, v, t0 : real) : vec3 = 2*g(u-0.5, v-0.5, t0);

plot f;
